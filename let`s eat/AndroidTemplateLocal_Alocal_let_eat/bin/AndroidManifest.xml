        <activity android:name=".user.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".user.RegisterActivity">
        </activity>
        <activity android:name=".WordActivity"></activity>
        <activity android:name=".user.HomeActivity"></activity>
        <activity android:name=".PlanAddActivity"></activity>
        <activity android:name=".PlanActivity"></activity>
        <activity android:name=".ReviewActivity"></activity>
        <activity android:name=".user.PasswordUpdateActivity"></activity>
    </application>

</manifest>